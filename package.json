{
  "name": "code-explorer",
  "displayName": "Code Explorer",
  "description": "Explore code efficiently with a stack",
  "version": "0.1.5",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/tianjianchn/code-explorer-vscode"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "publisher": "tianjianchn",
  "categories": [
    "Other"
  ],
  "keywords": [
    "Code Explorer",
    "Code Navigation",
    "Code Stack",
    "Read Code",
    "Code Tour"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "media/logo.png",
  "contributes": {
    "commands": [
      {
        "command": "codeExplorer.addMarker",
        "title": "Add Code Marker",
        "category": "Code Explorer"
      },
      {
        "command": "codeExplorer.markerTreeView.actions",
        "title": "Stack Actions",
        "category": "Code Explorer",
        "icon": "$(tools)",
        "enablement": "view == codeExplorer.markerTreeView"
      },
      {
        "command": "codeExplorer.markerTreeView.refresh",
        "title": "Refresh Stacks",
        "category": "Code Explorer",
        "icon": "$(refresh)"
      },
      {
        "command": "codeExplorer.markerTreeView.loadStack",
        "title": "Switch Stack",
        "category": "Code Explorer",
        "icon": "$(arrow-swap)"
      },
      {
        "command": "codeExplorer.markerTreeView.removeStack",
        "title": "Remove Current Stack",
        "category": "Code Explorer",
        "icon": "$(trash)"
      },
      {
        "command": "codeExplorer.markerTreeView.renameStack",
        "title": "Rename Current Stack",
        "category": "Code Explorer"
      },
      {
        "command": "codeExplorer.markerTreeView.clickMarker",
        "title": "Click Marker",
        "category": "Code Explorer"
      },
      {
        "command": "codeExplorer.markerTreeView.removeMarker",
        "title": "Remove Marker",
        "category": "Code Explorer",
        "icon": "$(close)",
        "enablement": "view == codeExplorer.markerTreeView"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "codeExplorerPanel",
          "title": "Code Explorer",
          "icon": "media/logo.svg"
        }
      ]
    },
    "views": {
      "debug": [],
      "codeExplorerPanel": [
        {
          "id": "codeExplorer.markerTreeView",
          "name": "Markers",
          "contextualTitle": "Code Explorer",
          "visibility": "visible"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "codeExplorer.addMarker",
          "group": "z_commands"
        }
      ],
      "view/title": [
        {
          "command": "codeExplorer.markerTreeView.loadStack",
          "when": "view == codeExplorer.markerTreeView",
          "group": "navigation"
        },
        {
          "command": "codeExplorer.markerTreeView.actions",
          "when": "view == codeExplorer.markerTreeView",
          "group": "navigation"
        },
        {
          "command": "codeExplorer.markerTreeView.refresh",
          "when": "view == codeExplorer.markerTreeView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "codeExplorer.markerTreeView.removeMarker",
          "when": "view == codeExplorer.markerTreeView && viewItem == marker",
          "group": "inline"
        },
        {
          "command": "codeExplorer.markerTreeView.removeMarker",
          "when": "view == codeExplorer.markerTreeView && viewItem == marker",
          "group": "0"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "rimraf ./out && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "1.80.0",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "@vscode/test-cli": "0.0.9",
    "@vscode/test-electron": "2.4.0",
    "eslint": "8.57.0",
    "rimraf": "5.0.7",
    "typescript": "5.4.5"
  }
}